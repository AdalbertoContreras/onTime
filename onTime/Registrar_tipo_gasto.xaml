<Window x:Class="onTime.Registrar_tipo_gasto"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:onTime"
        mc:Ignorable="d"
        Title="Registrar tipo gasto" Height="450" Width="400">
    <Window.Resources>
        <ResourceDictionary Source="resources/Dictionary.xaml" />
    </Window.Resources>
    <Grid>
        <StackPanel Orientation="Vertical">
            <TextBlock Text="Nombre de tipo de cuenta"/>
            <TextBox />
            <Border Style="{StaticResource BorderRegistro}">
                <Button Content="REGISTRAR TIPO DE CUENTA" Style="{StaticResource WindowButtonStyle}" />
            </Border>
        </StackPanel>
    </Grid>
</Window>
